package sec03.exam03;

public class ByteOperationExample {

	public static void main(String[] args) {
		
		byte result1 = 10 + 20;		//자바는 컴파일할때도 연산을 수행 함.
		
		
		byte x =10;
		byte y = 20;
		//byte result = x + y; 컴파일 오류 발생
		int result2 = x + y;
		
		//byte x2 = 30;
		//long y2 = 40;
		//long result3 = x2 + y2;
	}

}
